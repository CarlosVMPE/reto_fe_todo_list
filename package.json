    "@angular/router": "^16.0.0",
    "angular-google-tag-manager": "^1.8.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.5",
    "@angular/cli": "~16.0.5",
    "@angular/compiler-cli": "^16.0.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "jest-preset-angular": "^13.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^3.3.0",
    "typescript": "~5.0.2"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/config.jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.)": "<rootDir>/src/app/$1",
      "@src/(.)": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src/app/$1",
      "<rootDir>/src/$1"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
